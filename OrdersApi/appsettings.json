{
  "spring": {
    "application": {
      "name": "Orders API"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFethRegistry": true,
      "validateCertificates": false
    },
    "instance": {
      "port": 7000,
      "instanceId": "localhost:7000",
      "hostName": "localhost",
      "healthCheckUrlPath": "/get-orders",
      "statusPageUrlPath": "/get-orders"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}